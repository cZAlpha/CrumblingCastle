; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CrumblingCastle"
config/version="0.0"
config/tags=PackedStringArray("topdown", "zeldalike")
run/main_scene="res://Scenes/UI/intro_sequence.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://Assets/Images/MenuImages/CrumblingCastleNormal.png"

[autoload]

HealthManager="*res://Scripts/Managers/health_manager.gd"
SoundManager="*res://Scenes/Managers/sound_manager.tscn"
GameManager="*res://Scripts/Managers/game_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogue_manager]

general/balloon_path="res://Dialogue/balloon.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[global_group]

Player="The player's hitbox group."
Enemy="The group for general enemy hitboxes."
Sword="The player's sword's hitbox group."
Environment="A group made up of environment assets that collide with the player"
Door="Group for doors"
PlayerCollider="This group defines the player's collision shape group, NOT THE HITBOX, the hitbox for the player is usually the 'player' group."
PlayerAttack="A group used for hitboxes (hurtboxes) of the player's attacks. Can be used for any kind of attack from the player."
World="The group that includes any scene that encapsulates the player (i.e. Overworld, Inside of Buildings, Etc.)"
NPC="A group for NPC areas/hitboxes"

